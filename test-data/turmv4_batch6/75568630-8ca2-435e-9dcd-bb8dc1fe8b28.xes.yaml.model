<?xml version="1.0"?>
<testset xmlns="http://cpee.org/ns/properties/2.0">
  <executionhandler>ruby</executionhandler>
  <dataelements>
    <set>/Plc/DB100.DBX181.1</set>
    <check>/Plc/DB100.DBX164.5</check>
    <value>true</value>
  </dataelements>
  <endpoints>
    <opc_write>https-put://centurio.work/ing/opcua/write/</opc_write>
    <opc_read>https-get://centurio.work/ing/opcua/read/</opc_read>
  </endpoints>
  <description>
    <description xmlns="http://cpee.org/ns/description/1.0">
      <call id="a4" endpoint="opc_write">
        <parameters>
          <label>Set Var</label>
          <arguments>
            <endpoint_url>opc.tcp://OpcUaClient:SUNDOWN1@localhost:8011</endpoint_url>
            <namespace_index>2</namespace_index>
            <identifier_type>string</identifier_type>
            <identifier>!data.set</identifier>
            <value>!data.value.to_s</value>
          </arguments>
        </parameters>
        <code>
          <prepare/>
          <finalize output="result"/>
          <update output="result"/>
          <rescue output="result"/>
        </code>
        <annotations>
          <_timing>
            <_timing_weight/>
            <_timing_avg/>
            <explanations/>
          </_timing>
          <_shifting>
            <_shifting_type>Duration</_shifting_type>
          </_shifting>
          <_context_data_analysis>
            <probes/>
            <ips/>
          </_context_data_analysis>
          <report>
            <url/>
          </report>
          <_notes>
            <_notes_general/>
          </_notes>
        </annotations>
        <documentation>
          <input/>
          <output/>
          <implementation>
            <description/>
          </implementation>
          <code>
            <description/>
          </code>
        </documentation>
      </call>
      <loop mode="post_test" condition="data.value.to_s != data.check_value.to_s">
        <_probability>
          <_probability_min/>
          <_probability_max/>
          <_probability_avg/>
        </_probability>
        <call id="a6" endpoint="opc_read">
          <parameters>
            <label>Read Var</label>
            <arguments>
              <endpoint_url>opc.tcp://OpcUaClient:SUNDOWN1@localhost:8011</endpoint_url>
              <namespace_index>2</namespace_index>
              <identifier_type>string</identifier_type>
              <identifier>!data.check</identifier>
            </arguments>
          </parameters>
          <code>
            <prepare/>
            <finalize output="result">data.check_value = result.to_s</finalize>
            <update output="result"/>
            <rescue output="result"/>
          </code>
          <annotations>
            <_timing>
              <_timing_weight/>
              <_timing_avg/>
              <explanations/>
            </_timing>
            <_shifting>
              <_shifting_type>Duration</_shifting_type>
            </_shifting>
            <_context_data_analysis>
              <probes/>
              <ips/>
            </_context_data_analysis>
            <report>
              <url/>
            </report>
            <_notes>
              <_notes_general/>
            </_notes>
          </annotations>
          <documentation>
            <input/>
            <output/>
            <implementation>
              <description/>
            </implementation>
            <code>
              <description/>
            </code>
          </documentation>
        </call>
      </loop>
    </description>
  </description>
</testset>
